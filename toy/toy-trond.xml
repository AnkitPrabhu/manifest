<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="couchbase"
	  fetch="git://github.com/couchbase/"
	  review="review.couchbase.org" />
  <remote name="membase"
	  fetch="git://github.com/membase/"
	  review="review.couchbase.org" />
  <remote name="trond"
	  fetch="git://github.com/trondn/"
	  review="review.couchbase.org" />

  <default remote="couchbase" revision="master"/>

  <project name="cbsasl" path="source/cbsasl" />
  <project name="couchstore" path="source/couchstore"/>
  <project name="libconflate" path="source/libconflate" />
  <project name="libmemcached" path="source/libmemcached" remote="membase" revision="cmake" />
  <project name="libvbucket" path="source/libvbucket" />
  <project name="memcached" path="source/memcached" remote="membase"/>
  <project name="moxi" path="source/moxi"/>
  <project name="platform" path="source/platform" />
  <project name="ep-engine" path="source/ep-engine" />
  <project name="couchbase-cli" path="source/couchbase-cli" />
  <project name="couchbase-examples" path="source/couchbase-examples" />
  <project name="couchbase-python-client" path="source/couchbase-python-client" revision="couchbase-181" />
  <project name="healthchecker" path="source/healthchecker" />
  <project name="portsigar" path="source/portsigar" />
  <project name="sigar" path="source/sigar" />


  <!-- The rest of the modules not yet looked at -->
  <project name="couchdb" path="source/pending/couchdb" />
  <project name="geocouch" path="source/pending/geocouch"  />
  <project name="ns_server" path="source/pending/ns_server" revision="28d0ea57ee57b5bb4a214e3f7badf414a6ba744c" />

  <!-- The modules not referenced..  -->
  <project name="couchdbx-app" path="source/couchdbx-app" />
  <project name="testrunner" path="source/testrunner" />

  <project name="tlm" remote="trond" path="source/tlm">
    <copyfile src="CMakeLists.txt" dest="source/CMakeLists.txt" />
    <copyfile src="Makefile.pending" dest="source/Makefile.pending" />
    <copyfile src="Makefile.toplevel" dest="Makefile" />
  </project>

</manifest>
